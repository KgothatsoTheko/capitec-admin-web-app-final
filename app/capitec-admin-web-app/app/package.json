{
    "name": "admin-web-capitec",
    "version": "1.0.0",
    "studioVersion": "9.2.1",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "build-web": "node build-tasks/default.js && ng build --configuration=prod --aot --build-optimizer && node build-tasks/build-web.js",
        "build-mobile": "node build-tasks/default.js && ng build --base-href . --configuration=prod --aot --output-path \"www\"",
        "initialize-android": "node build-tasks/create-www.js && cordova platform add android",
        "initialize-ios": "node build-tasks/create-www.js && cordova platform add ios",
        "android-emulate": "npm run build-mobile && cordova emulate android",
        "android-build": "npm run build-mobile && cordova build android",
        "ios-emulate": "npm run build-mobile && cordova emulate ios",
        "ios-build": "npm run build-mobile && cordova build ios",
        "build-web-gcp": "node build-tasks/default.js && ng build --base-href /web --configuration=prod --aot --build-optimizer && node build-tasks/build-web.js"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "13.4.0",
        "@angular/cdk": "13.3.9",
        "@angular/common": "13.4.0",
        "@angular/compiler": "13.4.0",
        "@angular/core": "13.4.0",
        "@angular/flex-layout": "13.0.0-beta.38",
        "@angular/forms": "13.4.0",
        "@angular/material": "13.3.9",
        "@angular/platform-browser": "13.4.0",
        "@angular/platform-browser-dynamic": "13.4.0",
        "@angular/router": "13.4.0",
        "@firebase/app-compat": "0.2.13",
        "@swimlane/ngx-charts": "20.1.0",
        "android-versions": "1.5.0",
        "chart.js": "3.8.0",
        "core-js": "3.8.1",
        "hammerjs": "2.0.8",
        "localforage": "1.10.0",
        "moment": "2.29.4",
        "neutrinos-module": "0.0.70-beta.3",
        "neutrinos-oauth-client": "1.2.5-beta.1",
        "ng2-charts": "3.0.11",
        "rxjs": "7.5.5",
        "signature_pad": "4.0.7",
        "tslib": "2.4.0",
        "zone.js": "0.11.4"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "13.3.11",
        "@angular/cli": "13.3.11",
        "@angular/compiler-cli": "13.4.0",
        "@angular/language-service": "13.4.0",
        "@types/node": "12.12.42",
        "cordova-config": "0.7.0",
        "typescript": "4.6.4"
    }
}
